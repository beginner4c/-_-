#include<stdio.h>

main() {
	int i;
	int max = 0;
	int arr[9];
	int num = 0;
	max = arr[0];
	for (i = 0; i < 9; i++) {
		scanf(" %d", &arr[i]);
		if (arr[i] >= 100)
		{
			return 0;
		}
		if (max < arr[i])
		{
			max = arr[i];
			num = i + 1;//->이게 겁나 생각이 안나서 4번틀림
		}
	}
	printf("%d\n%d", max, num);
}
